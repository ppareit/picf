<schemalist>
  <schema id="be.ppareit.picf" path="/be/ppareit/picf/" gettext-domain="picf">

    <key name="path" type="s">
      <default>''</default>
      <summary>Path to the picture to display</summary>
    </key>

    <key name="position" type="(ii)">
        <default>(0,0)</default>
        <summary>Position on the screen</summary>
    </key>

    <key name="size" type="(ii)">
        <default>(320,240)</default>
        <summary>Size of the frame</summary>
    </key>

  </schema>
</schemalist>
